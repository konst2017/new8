{
  "name": "yiimaker/yii2-newsletter",
  "description": "Module for saving user contacts from newsletter form to database",
  "keywords": ["yii2", "module", "newsletter", "subscribe"],
  "type": "yii2-extension",
  "license": "BSD-3-Clause",
  "support": {
    "source": "https://github.com/yiimaker/yii2-newsletter",
    "issues": "https://github.com/yiimaker/yii2-newsletter/issues"
  },
  "authors": [
    {
      "name": "Vladimir Kuprienko",
      "email": "vldmr.kuprienko@gmail.com",
      "role": "Developer"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "yiisoft/yii2": "^2.0.0",
    "yiisoft/yii2-bootstrap": "~2.0.0"
  },
  "require-dev": {
    "codeception/codeception": "~2.2",
    "codeception/verify": "*",
    "codeception/specify": "*"
  },
  "autoload": {
    "psr-4": {
      "ymaker\\newsletter\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ymaker\\newsletter\\tests\\": "tests"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.1.x-dev"
    }
  }
}